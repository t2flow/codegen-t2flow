{
  "name": "codegen",
  "version": "1.0.0",
  "description": "Parses a JSON DAG and produces tensorflowjs executable code snippets",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "moduleFileExtensions":[
      "ts", 
      "js"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.spec\\.ts$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t2flow/codegen-t2flow.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/t2flow/codegen-t2flow/issues"
  },
  "homepage": "https://github.com/t2flow/codegen-t2flow#readme",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  }
}
